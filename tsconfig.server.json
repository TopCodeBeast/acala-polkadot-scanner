{
    "display": "Node 12",
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "./server",
        "moduleResolution": "node",
        "sourceMap": true
    },
    "compileOnSave": true,
    "exclude": [
        "./src"
    ],
    "include": [
        "server.ts"
    ]
}